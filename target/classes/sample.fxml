<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="390.0" prefWidth="604.0" stylesheets="@Viper.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dm.Application.UIThread">
   <children>
      <HBox>
         <children>
            <TextField fx:id="field" prefHeight="36.0" prefWidth="430.0" promptText="URL here">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" />
               </padding></TextField>
            <Button fx:id="start" mnemonicParsing="false" onAction="#onStartButtonClicked" prefHeight="36.0" prefWidth="72.0" text="START" />
         </children>
         <VBox.margin>
            <Insets bottom="8.0" left="45.0" right="8.0" top="8.0" />
         </VBox.margin>
      </HBox>
      <HBox fx:id="viewArea" prefHeight="300.0" prefWidth="577.0">
         <VBox.margin>
            <Insets left="10.0" top="20.0" />
         </VBox.margin>
         <children>
            <TableView fx:id="view" fixedCellSize="0.0" prefHeight="292.0" prefWidth="584.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <columns>
                  <TableColumn fx:id="name" prefWidth="279.0" text="Name" />
                  <TableColumn fx:id="Progress" prefWidth="304.0" text="Progress" />
               </columns>
            </TableView>
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>

</VBox>
